{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run eval_agent.py with args",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/protoverse/eval_agent.py",
            "console": "integratedTerminal",
            "args": [
                "+robot=g1",
                "+simulator=isaaclab",
                "+checkpoint=results/g1_follow/last.ckpt"
            ],
            "env": {
                "WANDB_DISABLE_SENTRY": "true",
                "WANDB_SILENT": "true",
                "WANDB_DISABLE_CODE": "true"
            }
        }
    ]
}